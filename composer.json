{
    "name": "inmanturbo/quickweb-settings",
    "description": "The Quickweb settings editor",
    "keywords": [
        "laravel",
        "settings",
        "settings-editor"
    ],
    "type": "ui",
    "license": "MIT",
    "authors": [
        {
            "name": "inmanturbo",
            "email": "inman.turbo@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "laravel/helpers": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Quickweb\\SettingsEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Quickweb\\SettingsEditor\\SettingsEditorServiceProvider"
            ],
            "aliases": {
                "DotenvEditor": "Quickweb\\SettingsEditor\\SettingsEditorFacade"
            }
        }
    }
}